   "metadata": {},
   "outputs": [],
   "source": [
    "from scipy.stats import poisson"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "ebf03e43-9624-4280-aa1f-aaf408f53343",
   "metadata": {},
   "outputs": [],
   "source": [
